/* Private user code ---------------------------------------------------------*/
/* USER CODE BEGIN 0 */

void PCA9685_SetBit(uint8_t devAddr, uint8_t Register, uint8_t Bit, uint8_t Value)
{
  uint8_t readValue;
  HAL_StatusTypeDef ret;

  // Read register
  ret = HAL_I2C_Mem_Read(&hi2c1, devAddr, Register, 1, &readValue, 1, 10);
  if (ret != HAL_OK) {
	  printf("I2C FAIL: Read bit reg 0x%02X\n", Register);
	  return;
  }

  // Read all 8 bits and set only one bit to 0/1 and write all 8 bits back
  HAL_I2C_Mem_Read(&hi2c1, devAddr, Register, 1, &readValue, 1, 10);
  if (Value == 0) readValue &= ~(1 << Bit);
  else readValue |= (1 << Bit);
  HAL_I2C_Mem_Write(&hi2c1, devAddr, Register, 1, &readValue, 1, 10);
  HAL_Delay(1);
}

void PCA9685_SetPWMFrequency(uint8_t devAddr, uint16_t frequency)
{
	uint8_t prescale;
	  if(frequency >= 1526) prescale = 0x03;
	  else if(frequency <= 24) prescale = 0xFF;
	  //  internal 25 MHz oscillator as in the datasheet page no 1/52
	  else prescale = 25000000 / (4096 * frequency);
	  // prescale changes 3 to 255 for 1526Hz to 24Hz as in the datasheet page no 1/52
	  PCA9685_SetBit(devAddr, PCA9685_MODE1, PCA9685_MODE1_SLEEP_BIT, 1);
	  HAL_I2C_Mem_Write(&hi2c1, devAddr, PCA9685_PRE_SCALE, 1, &prescale, 1, 10);
	  PCA9685_SetBit(devAddr, PCA9685_MODE1, PCA9685_MODE1_SLEEP_BIT, 0);
	  PCA9685_SetBit(devAddr, PCA9685_MODE1, PCA9685_MODE1_RESTART_BIT, 1);
}

void PCA9685_Init(uint8_t devAddr, uint16_t frequency)
{
	PCA9685_SetPWMFrequency(devAddr, frequency); // 50 Hz for servo
	PCA9685_SetBit(devAddr, PCA9685_MODE1, PCA9685_MODE1_AI_BIT, 1);

}

void PCA9685_SetPWM(uint8_t devAddr, uint8_t Channel, uint16_t OnTime, uint16_t OffTime)
{
  uint8_t registerAddress;
  uint8_t pwm[4];
  registerAddress = PCA9685_LED0_ON_L + (4 * Channel);
  // See example 1 in the datasheet page no 18/52
  pwm[0] = OnTime & 0xFF;
  pwm[1] = OnTime>>8;
  pwm[2] = OffTime & 0xFF;
  pwm[3] = OffTime>>8;
  HAL_StatusTypeDef ret = HAL_I2C_Mem_Write(&hi2c1, devAddr, registerAddress, 1, pwm, 4, 10);

  if (ret != HAL_OK) {
	  printf("I2C FAIL: PWM write ch %d\n", Channel);
  }
}

void PCA9685_SetServoAngle(uint8_t devAddr, uint8_t Channel, float Angle)
{
  float Value;
  // 50 Hz servo then 4095 Value --> 20 milliseconds
  // 0 degree --> 0.5 ms(102.4 Value) and 180 degree --> 2.5 ms(511.9 Value)
  Value = (Angle * (511.9 - 102.4) / 180.0) + 102.4;
  PCA9685_SetPWM(devAddr, Channel, 0, (uint16_t)Value);
}

void PCA9685_SetOutputMode(uint8_t devAddr) {
    uint8_t mode2;
    HAL_I2C_Mem_Read(&hi2c1, devAddr, 0x01, 1, &mode2, 1, 10);
    mode2 |= 0x04;
    HAL_I2C_Mem_Write(&hi2c1, devAddr, 0x01, 1, &mode2, 1, 10);
}


void microDelay (uint16_t delay)
{
  __HAL_TIM_SET_COUNTER(&htim3, 0);
  while (__HAL_TIM_GET_COUNTER(&htim3) < delay);
}

static inline void delayMicroseconds(uint16_t us) __attribute__((always_inline));

static inline void delayMicroseconds(uint16_t us)
{
    __HAL_TIM_SET_COUNTER(&htim3, 0);
    while (__HAL_TIM_GET_COUNTER(&htim3) < us);
}

void delayMilliseconds(uint32_t ms)
{
    // For each millisecond, delay 1000 microseconds
    for (uint32_t i = 0; i < ms; i++) {
        delayMicroseconds(1000);  // 1ms = 1000Âµs
    }
}

