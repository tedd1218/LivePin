/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_I2C1_Init();
  MX_TIM3_Init();
  /* USER CODE BEGIN 2 */

  // PCA9685_Init(PCA9685_ADDRESS, 50);
  HAL_TIM_Base_Start(&htim3);
  HAL_GPIO_WritePin(DIR_PORT_L, DIR_PIN_L, GPIO_PIN_SET);
  delayMilliseconds(100);
  HAL_GPIO_WritePin(DIR_PORT_L, DIR_PIN_L, GPIO_PIN_RESET);
  delayMilliseconds(100);

  HAL_GPIO_WritePin(DIR_PORT_R, DIR_PIN_R, GPIO_PIN_SET);
  delayMilliseconds(100);
  HAL_GPIO_WritePin(DIR_PORT_R, DIR_PIN_R, GPIO_PIN_RESET);
  delayMilliseconds(100);

  STOP_STEPPERS = 0;
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
	  // reset actuators extend
	  HAL_GPIO_WritePin(RESET_ACTUATOR_R1_PORT, RESET_ACTUATOR_R1_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B1_PORT, RESET_ACTUATOR_B1_PIN, GPIO_PIN_RESET);

	  HAL_GPIO_WritePin(RESET_ACTUATOR_R2_PORT, RESET_ACTUATOR_R2_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B2_PORT, RESET_ACTUATOR_B2_PIN, GPIO_PIN_RESET);

	  // wait for them to extend
	  HAL_Delay(5000);

	  // reset actuators retract
	  HAL_GPIO_WritePin(RESET_ACTUATOR_R1_PORT, RESET_ACTUATOR_R1_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B1_PORT, RESET_ACTUATOR_B1_PIN, GPIO_PIN_RESET);

	  HAL_GPIO_WritePin(RESET_ACTUATOR_R2_PORT, RESET_ACTUATOR_R2_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B2_PORT, RESET_ACTUATOR_B2_PIN, GPIO_PIN_RESET);

	  // wait for them to retract
	  HAL_Delay(5000);

	  // reset actuators extend
	  HAL_GPIO_WritePin(RESET_ACTUATOR_R1_PORT, RESET_ACTUATOR_R1_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B1_PORT, RESET_ACTUATOR_B1_PIN, GPIO_PIN_RESET);

	  HAL_GPIO_WritePin(RESET_ACTUATOR_R2_PORT, RESET_ACTUATOR_R2_PIN, GPIO_PIN_SET);
	  HAL_GPIO_WritePin(RESET_ACTUATOR_B2_PORT, RESET_ACTUATOR_B2_PIN, GPIO_PIN_RESET);
	  // interrupt to stop everything
//	  if (STOP_STEPPERS) {
//		  PCA9685_SetServoAngle(PCA9685_ADDRESS, 0, 0);
//		  PCA9685_SetServoAngle(PCA9685_ADDRESS2, 0, 0);
//		  while (1);
//	  }

	  // receiving UART info
//	  HAL_StatusTypeDef status = HAL_UART_Receive(&huart2, rx_buffer, sizeof(rx_buffer), HAL_MAX_DELAY);
//
//	  if (status == HAL_OK)
//	  {
//		  for (int i = 0; i < ROWS * COLS; i++)
//		  {
//			  memcpy(&depth[i / COLS][i % COLS], &rx_buffer[i * 4], sizeof(float));
//		  }
//		  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_0, GPIO_PIN_SET);
//		  HAL_Delay(200);
//		  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_0, GPIO_PIN_RESET);
//		  HAL_Delay(200);
//		  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_0, GPIO_PIN_SET);
//		  HAL_Delay(200);
//		  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_0, GPIO_PIN_RESET);
//	  }

	  // servo motor spin
	  // pwm expander servos
//	  for (int i = 0; i < 16; i++) {
//		  PCA9685_SetServoAngle(PCA9685_ADDRESS, i, 0);
//	  }



	  // gantry movement
	  // delayMilliseconds(1000);

	  // moving down
//	  HAL_GPIO_WritePin(DIR_PORT_L, DIR_PIN_L, GPIO_PIN_SET);
//	  HAL_GPIO_WritePin(DIR_PORT_R, DIR_PIN_R, GPIO_PIN_RESET);


	  // moving up
//	  HAL_GPIO_WritePin(DIR_PORT_L, DIR_PIN_L, GPIO_PIN_RESET);
//	  HAL_GPIO_WritePin(DIR_PORT_R, DIR_PIN_R, GPIO_PIN_SET);
//	  // delayMicroseconds(20000);
//	  for (int i = 0; i < 10000; i++) {
//
//		  if (STOP_STEPPERS) {
//			  break;
//		  }
//		  HAL_GPIO_WritePin(STEP_PORT_L, STEP_PIN_L, GPIO_PIN_SET);
//		  HAL_GPIO_WritePin(STEP_PORT_R, STEP_PIN_R, GPIO_PIN_SET);
//		  delayMicroseconds(100);
//		  HAL_GPIO_WritePin(STEP_PORT_L, STEP_PIN_L, GPIO_PIN_RESET);
//		  HAL_GPIO_WritePin(STEP_PORT_R, STEP_PIN_R, GPIO_PIN_RESET);
//		  delayMicroseconds(100);
//	  }

    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{
  RCC_OscInitTypeDef RCC_OscInitStruct = {0};
  RCC_ClkInitTypeDef RCC_ClkInitStruct = {0};

  /** Configure the main internal regulator output voltage
  */
  __HAL_RCC_PWR_CLK_ENABLE();
  __HAL_PWR_VOLTAGESCALING_CONFIG(PWR_REGULATOR_VOLTAGE_SCALE2);

  /** Initializes the RCC Oscillators according to the specified parameters
  * in the RCC_OscInitTypeDef structure.
  */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_HSI;
  RCC_OscInitStruct.HSIState = RCC_HSI_ON;
  RCC_OscInitStruct.HSICalibrationValue = RCC_HSICALIBRATION_DEFAULT;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_ON;
  RCC_OscInitStruct.PLL.PLLSource = RCC_PLLSOURCE_HSI;
  RCC_OscInitStruct.PLL.PLLM = 8;
  RCC_OscInitStruct.PLL.PLLN = 80;
  RCC_OscInitStruct.PLL.PLLP = RCC_PLLP_DIV2;
  RCC_OscInitStruct.PLL.PLLQ = 7;
  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    Error_Handler();
  }

  /** Initializes the CPU, AHB and APB buses clocks
  */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_PLLCLK;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV1;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV2;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_2) != HAL_OK)
  {
    Error_Handler();
  }
}

